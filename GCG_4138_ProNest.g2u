Program.Sub.PartsCompleted.Start
V.Local..BulkDeclareString(sWC, sSql, sWO, sSuff)
V.Local.iC.Declare(Long)
V.Local.fUnits.Declare(Float)
V.Local.fTotal.Declare(Float)

F.Intrinsic.String.Build("{0}01", V.Global.sLoc, V.Local.sWC)

'F.Intrinsic.Debug.InvokeDebugger

F.Intrinsic.Debug.Stop

'Here we are going to be assigning how many parts were made to the sequence with the work center is either 1001 if location 10 or 2001 if location 20
F.Intrinsic.Control.For(V.Local.iC, 0, V.DataView.ProNest!Nest.RowCount--,1)
	V.Local.fUnits.Set(V.DataView.ProNest!Nest(V.Local.iC).QTYNEST!FieldVal)
	V.Local.sWO.Set(V.DataView.ProNest!Nest(V.Local.iC).WO#!FieldVal)
	F.Intrinsic.String.Split(V.Local.sWO, "-", V.Local.sWO)
'	V.Local.sSql.Set("UPDATE JOB_OPERATIONS_WC SET FLAG_CLOSED = 'Y', UNITS_COMPLETE = ")
'	F.Intrinsic.String.Build("{0}'{1}' where WORKCENTER = '{2}' and JOB = '{3}' and SUFFIX = '{4}' ", V.Local.sSql, V.Local.fUnits, V.Local.sWC, V.Local.sWO(0), V.Local.sWO(1), V.Local.sSql)
'	F.ODBC.Connection!Conx.Execute(V.Local.sSql)
	V.Local.sSql.Set("Select FLAG_CLOSED, UNITS_COMPLETE, UNITS_OPEN from JOB_OPERATIONS_WC where ")
	F.Intrinsic.String.Build("{0}WORKCENTER = '{1}' and JOB = '{2}' and SUFFIX = '{3}' ", V.Local.sSql, V.Local.sWC, V.Local.sWO(0), V.Local.sWO(1), V.Local.sSql)
	F.ODBC.Connection!Conx.OpenLocalRecordsetRW("closerst", V.Local.sSql)
	F.Intrinsic.Control.DoUntil(V.ODBC.Conx!closerst.EOF, =, True)
		F.Intrinsic.Math.Add(V.ODBC.Conx!closerst.FieldValLong!UNITS_COMPLETE, V.Local.fUnits, V.Local.fTotal)
		F.Intrinsic.Control.If(V.ODBC.Conx!closerst.FieldValLong!UNITS_OPEN, <=, V.Local.fTotal)
			F.ODBC.Conx!closerst.Set!FLAG_CLOSED("Y")
		F.Intrinsic.Control.EndIf
		F.ODBC.Conx!closerst.Set!UNITS_COMPLETE(V.Local.fTotal)
		F.ODBC.Conx!closerst.Update	
		F.ODBC.Conx!closerst.MoveNext
	F.Intrinsic.Control.Loop
	F.ODBC.Conx!closerst.Close
F.Intrinsic.Control.Next(V.Local.iC)

Program.Sub.PartsCompleted.End

